eWarningCount":1,"source":"72"},"/Users/vczaran/Desktop/WildWatch/src/index.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/App.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/Modal/Modal.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/NavBar/NavBar.js",["73"],[],"import { useState } from 'react'\nimport './NavBar.css'\nimport { Link } from 'react-router-dom';\nimport TutorialModal from '../TutorialModal/TutorialModal';\nimport AboutUsModal from '../AboutUsModal/AboutUsModal';\n\nexport default function NavBar () {\n    const [modalOpen, setModalOpen] = useState(true);\n    const [modalType, setModalType] = useState(\"tutorial\");\n\n    const openModal = () => {\n        setModalOpen(true)\n    }\n\n    const closeModal = () => {\n        setModalOpen(false)\n    }\n\n    return (\n        <>\n        <div \n            className=\"nav-bar\">\n            <Link \n                to='/' \n                className='nav-link'>\n                <h1 \n                    id=\"nav-logo\">\n                        WILDWATCH\n                </h1>\n            </Link>\n            <Link \n                to=\"/species\" \n                className=\"nav-link\">\n                <i \n                    className=\"fa-solid fa-otter fa-2xl icon\" \n                    style={{ color: \"#1f2f4d\" }} />\n                    &nbsp; Species List\n            </Link>\n\n            <div \n                id=\"nav-tutorial\" \n                onClick={() => {\n                    setModalType(\"tutorial\");\n                    setModalOpen(true);\n                }}>\n                <i \n                    className=\"fa-solid fa-circle-info fa-2xl icon\" \n                    style={{color: \"#1f2f4d\"}} />\n                    &nbsp; Tutorial\n            </div>\n            {modalOpen && modalType === \"tutorial\" &&\n                <TutorialModal\n                    closeModal={closeModal}/>}   \n            <div \n                id=\"nav-about-us\" \n                onClick={() => {\n                    setModalOpen(true);\n                    setModalType(\"about-us\")\n                }}>\n                <i \n                    className=\"fa-solid fa-user\"\n                    style={{ color: \"#1f2f4d\" }} />\n                    &nbsp; About\n            </div>\n            {modalOpen && modalType === \"about-us\" &&\n                <AboutUsModal \n                    closeModal={closeModal}/>}   \n        </div>\n        </>\n    )\n}","/Users/vczaran/Desktop/WildWatch/src/components/Globe/GlobeComponent.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/SpeciesList/SpeciesList.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/Globe/Globe.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/Globe/data.js",[],[],"/Users/vczaran/Desktop/WildWatch/src/components/SpeciesList/SpeciesItem.js",["74","75"],[],"import './SpeciesList.css'\n\nexport default function SpeciesItem ({animal}) {\n    const name = animal.species\n    const continent = animal.continent\n    const foundation = animal.foundation\n    const link = animal.url\n    const photo = animal.photo\n\n    console.log(link)\n    return(\n        <>\n            <div className=\"animal-item\">\n                <img className='animal-image' src={photo}/>\n                <div className='animal-description'>\n                    <h2>\n                        {name}\n                    </h2>\n                    <div>\n                        Continent: {continent}\n                    </div>\n                    <br></br>\n                    <div>\n                        Foundation: <br></br>\n                        <a href={link} target=\"_blank\" >\n                            {foundation}\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </>\n    )\n}","/Users/vczaran/Desktop/WildWatch/src/components/TutorialModal/TutorialModal.js",["76","77"],[],"import { useState } from \"react\";\nimport { Modal } from \"../Modal/Modal\";\nimport \"./TutorialModal.css\";\nimport gif from \"../../assets/tutorial.gif\"\n\nconst TutorialModal = (props) => {\n    const { closeModal } = props;\n    const [modalOpen, setModalOpen] = useState(false)\n\n    return (\n        <Modal\n            onClose={closeModal}>\n                <h1>Tutorial:</h1>\n                <img src={gif} alt=\"gif\"/>\n                <p>1. Click and hold the cursor to rotate the globe</p>\n                <p>2. Release and click on desired continent to view three of the most endangered species</p>\n                <p>3. Click foundation links to be redirected</p>\n                <p>4. Click anywhere outside of the modal to close</p>\n\n        </Modal>\n    )\n}\n\nexport default TutorialModal;","/Users/vczaran/Desktop/WildWatch/src/components/AboutUsModal/AboutUsModal.js",["78","79","80","81","82"],[],"import { useState } from \"react\";\nimport { Modal } from \"../Modal/Modal\";\nimport \"./AboutUsModal.css\"\n\nconst AboutUsModal = (props) => {\n    const { closeModal } = props;\n    const [modalOpen, setModalOpen] = useState(false)\n\n    return (\n        <Modal\n            onClose={closeModal}>\n                <div\n                    className=\"about-page\">\n\n                    <h1>About the App</h1>\n                    <p className=\"app-desc\">\n                        Across the world, countless animals are in danger of extinction. <br></br>\n                        With over 42,000 species listed as endangered on the IUCN Red List,<br></br>\n                        it can be difficult to determine how to help. <br></br> \n                        {/* That’s where WildWatch comes in – clicking on any of the continents on our interactive globe \n                        will open a window that displays three of the most endangered animals \n                        on that continent. <br></br> */}\n                        Links to species-specific charities are listed underneath each animal, <br></br>\n                        helping animal lovers around the world better focus their conservation efforts. <br></br>\n                        All charities are verified for legitimacy and impact using Charity Navigator <br></br>\n                        (https://www.charitynavigator.org / https://www.charitynavigator.org) <br></br>\n                        so you can be assured that your contribution will go to the animals in need!</p>\n                    <h1>About the Team</h1>\n                    <div \n                        className=\"info-container\">\n                        <div id=\"individual-info\">\n                            <img className=\"michelle\" src=\"/michelle.png\"/>\n                            <div id=\"individual-subinfo\">\n                                <h2>Michelle Li</h2>\n                                <div id=\"social-links\">\n                                    <a href=\"https://github.com/michelleeli\"><i className=\"fa-brands fa-github fa-2xl\" style={{color: \"#000000\"}}></i></a>\n                                    <a href=\"https://www.linkedin.com/in/michelle-li-040922288/\"><i className=\"fa-brands fa-linkedin fa-2xl\" style={{color: \"#0a66c2\"}}></i></a>\n                                </div>\n                            </div>\n                        </div>\n                        <div id=\"individual-info\">\n                            <img className=\"viktoria\" src=\"/viktoria.png\"/>\n                            <div id=\"individual-subinfo\">\n                                <h2>Viktoria Czaran</h2>\n                                <div id=\"social-links\">\n                                    <a href=\"https://github.com/vczaran\"><i className=\"fa-brands fa-github fa-2xl\" style={{color: \"#000000\"}}></i></a>\n                                    <a href=\"https://www.linkedin.com/in/viktoria-czaran-4688ab284/\"><i className=\"fa-brands fa-linkedin fa-2xl\" style={{color: \"#0a66c2\"}}></i></a>\n                                </div>\n                            </div>\n                        </div>\n                        <div id=\"individual-info\">\n                            <img className=\"ashley\" src=\"/ashley.png\"/>\n                            <div id=\"individual-subinfo\">\n                                <h2>Ashley Kim</h2>\n                                <div id=\"social-links\">\n                                    <a href=\"https://github.com/ashleyjek\"><i className=\"fa-brands fa-github fa-2xl\" style={{color: \"#000000\"}}></i></a>\n                                    <a href=\"https://www.linkedin.com/in/ashleyjek/\"><i className=\"fa-brands fa-linkedin fa-2xl\" style={{color: \"#0a66c2\"}}></i></a>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n        </Modal>\n    )\n}\n\nexport default AboutUsModal;","/Users/vczaran/Desktop/WildWatch/src/components/ContinentModal/ContinentModal.js",["83","84"],[],"import { Modal } from \"../Modal/Modal\";\nimport \"./ContinentModal.css\"\nimport SpeciesItemModal from \"./SpeciesItemModal\";\nimport { useEffect, useState } from \"react\";\n\nexport default function ContinentModal (props) {\n    const { closeModal, continent } = props;\n    const [data, setData] = useState([]);\n\n    const getData = () => {\n        fetch('SpeciesByCont.json', {\n            headers: {\n                'Content-Type': 'application/json',\n                'Accept': 'application/json'\n            }\n        }).then(function(resp) {\n            return resp.json();\n        }).then((myJson) => {\n            let data = myJson.filter((species) => species.continent === continent)\n            setData(data)\n        });\n    };\n\n    useEffect(() => {\n        getData(continent);\n    }, continent);\n\n    return (\n        <Modal onClose={closeModal}>\n            <h2 id=\"continent-title\">{continent}</h2>\n            <div id=\"species-grid\">\n                { data.map((species) => {\n                    return (\n                        <SpeciesItemModal species={species}/>\n                    )\n                })}\n            </div>\n        </Modal>\n    )\n}","/Users/vczaran/Desktop/WildWatch/src/components/ContinentModal/SpeciesItemModal.js",["85","86"],[],"const SpeciesItemModal = ({species}) => {\n    const name = species.species;\n    const foundation = species.foundation;\n    const link = species.url;\n    const photo = species.photo;\n\n    return (\n        <>\n            <div \n                className='species-item'>\n                <img \n                    className='species-image'\n                    src={photo}/>\n                <div \n                    className='species-desc'>\n                        <h2> {name} </h2>\n                        <h2> Foundation: <br></br>\n                            <a \n                                className='species-link'\n                                href={link} \n                                target='_blank'>\n                                    {foundation}\n                            </a> \n                        </h2>\n                    </div>\n            </div>\n        </>\n    )\n}\n\nexport default SpeciesItemModal;",{"ruleId":"87","severity":1,"message":"88","line":11,"column":11,"nodeType":"89","messageId":"90","endLine":11,"endColumn":20},{"ruleId":"91","severity":1,"message":"92","line":14,"column":17,"nodeType":"93","endLine":14,"endColumn":60},{"ruleId":"94","severity":1,"message":"95","line":25,"column":25,"nodeType":"93","messageId":"96","endLine":25,"endColumn":57,"fix":"97"},{"ruleId":"87","severity":1,"message":"98","line":8,"column":12,"nodeType":"89","messageId":"90","endLine":8,"endColumn":21},{"ruleId":"87","severity":1,"message":"99","line":8,"column":23,"nodeType":"89","messageId":"90","endLine":8,"endColumn":35},